{
  "version": 2,
  "name": "ciphertalk-frontend",
  "builds": [
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/client/(.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/",
      "dest": "/client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/",
      "destination": "/client/index.html"
    },
    {
      "source": "/((?!client).*)",
      "destination": "/client/$1"
    }
  ]
}